-
  const items = [
  {
  name: 'Element 1',
  path: 'javascript:void(0)'
  },
  {
  name: 'Element 2',
  path: 'lazy.html'
  },
  {
  name: 'Element 3',
  path: 'javascript:void(0)'
  },
  {
  name: 'Element 4',
  path: 'javascript:void(0)'
  },
  {
  name: 'Element 5',
  path: 'javascript:void(0)'
  }
  ]

.nav
  .mob-close
    +icon('close')
  nav
    ul
      each item in items
        li
          a(href="" + item.path class=(item.path === settings.titles[settings.titles.length - 1].path ? 'selected' : ''))= item.name
          if item.items
            ul
              each item in item.items
                li
                  a(href="" + item.path class=(item.path === settings.titles[settings.titles.length - 1].path ? 'selected' : ''))= item.name
